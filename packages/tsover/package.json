{
  "name": "tsover",
  "type": "commonjs",
  "version": "0.0.0",
  "description": "Fork of TypeScript with operator overloading",
  "keywords": [
    "operator-overloading",
    "typescript",
    "unplugin",
    "vite-plugin",
    "webpack-plugin",
    "rollup-plugin",
    "esbuild-plugin"
  ],
  "license": "MIT",
  "bin": {
    "tsc": "./bin/tsc",
    "tsserver": "./bin/tsserver"
  },
  "files": [
    "dist",
    "bin",
    "lib",
    "README.md",
    "LICENSE",
    "!**/.gitattributes"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./typescript": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "default": "./dist/plugin/index.js"
    },
    "./plugin/vite": {
      "types": "./dist/plugin/vite.d.ts",
      "default": "./dist/plugin/vite.js"
    },
    "./plugin/webpack": {
      "types": "./dist/plugin/webpack.d.ts",
      "default": "./dist/plugin/webpack.js"
    },
    "./plugin/rollup": {
      "types": "./dist/plugin/rollup.d.ts",
      "default": "./dist/plugin/rollup.js"
    },
    "./plugin/esbuild": {
      "types": "./dist/plugin/esbuild.d.ts",
      "default": "./dist/plugin/esbuild.js"
    },
    "./plugin/rspack": {
      "types": "./dist/plugin/rspack.d.ts",
      "default": "./dist/plugin/rspack.js"
    }
  },
  "scripts": {
    "patch": "bun scripts/patch.ts v5.9.3 && cp -R typescript/bin ./ && cp -R typescript/built/local ./lib",
    "build": "tsdown",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "minimatch": "^9.0.0",
    "unplugin": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.0.0"
  }
}
