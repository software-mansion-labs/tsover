{
  "name": "tsover",
  "version": "5.9.0",
  "description": "Fork of TypeScript with operator overloading",
  "keywords": [
    "esbuild-plugin",
    "operator-overloading",
    "rollup-plugin",
    "typescript",
    "unplugin",
    "vite-plugin",
    "webpack-plugin"
  ],
  "license": "Apache-2.0",
  "bin": {
    "tsc": "./bin/tsc",
    "tsserver": "./bin/tsserver"
  },
  "files": [
    "dist",
    "bin",
    "lib",
    "README.md",
    "LICENSE",
    "!**/.gitattributes"
  ],
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript.d.ts",
      "default": "./lib/typescript.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.cjs"
    },
    "./plugin/esbuild": {
      "types": "./dist/plugin/esbuild.d.ts",
      "default": "./dist/plugin/esbuild.cjs"
    },
    "./plugin/rollup": {
      "types": "./dist/plugin/rollup.d.ts",
      "default": "./dist/plugin/rollup.cjs"
    },
    "./plugin/rspack": {
      "types": "./dist/plugin/rspack.d.ts",
      "default": "./dist/plugin/rspack.cjs"
    },
    "./plugin/vite": {
      "types": "./dist/plugin/vite.d.ts",
      "default": "./dist/plugin/vite.cjs"
    },
    "./plugin/webpack": {
      "types": "./dist/plugin/webpack.d.ts",
      "default": "./dist/plugin/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "patch": "bun scripts/patch.ts v5.9.3 && rm -rf ./bin && rm -rf ./lib && cp -R typescript/bin ./ && cp -R typescript/built/local ./lib && cp typescript/LICENSE.txt ./lib && cp typescript/LICENSE.txt ./bin",
    "build": "tsdown",
    "test": "pnpm run build && vitest",
    "prepublishOnly": "pnpm run patch && pnpm run build && vitest run"
  },
  "dependencies": {
    "minimatch": "^9.0.0",
    "unplugin": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "comment-json": "^4.5.1",
    "tsdown": "^0.20.3",
    "tsover": "workspace:*",
    "tsover-runtime": "workspace:*",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
