{
  "name": "tsover-polyfill",
  "version": "0.0.1",
  "description": "Polyfill for TypeScript operator overloading",
  "keywords": [
    "operator-overloading",
    "polyfill",
    "typescript",
    "vite-plugin"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "minimatch": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "tsover": "*",
    "vite": "^4.0.0 || ^5.0.0"
  }
}
